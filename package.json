{
  "name": "electron-fullstack-quick-start-vuetify",
  "displayName": "Electron Fullstack Quick Start Vuetify",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron Fullstack Quick Start with Vuetify as Frontend",
  "author": "Muhammad Aufa Rijal <rijalaufa86@gmail.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/aufarijaal/electron-fullstack-quick-start-vuetify",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "dayjs": "^1.11.7",
    "knex": "^2.4.2",
    "pinia": "^2.0.33",
    "sqlite3": "^5.1.6",
    "vuetify": "^3.1.11",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@matpool/vue-json-view": "^0.1.8",
    "@mdi/font": "^7.2.96",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^3.1.2",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.15.2",
    "vite": "^3.2.2",
    "vite-plugin-electron": "^0.10.4",
    "vitest": "^0.29.7",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.9"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "vuetify"
  ]
